    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    }
    stages {
        stage('Build') {
            steps {
                echo 'building'
            }
        }
    }
    stages {
        stage('Testing') {
            steps {
                echo 'Testing'
            }
        }
    }
    stages {
        stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
    }
}
